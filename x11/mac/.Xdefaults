! *foreground:        White
URxvt.perl-lib:     /Users/myuser/.urxvt
URxvt*chdir:        /Users/myuser/git

urxvt*font:         xft:terminus:pixelsize=14,xft:Inconsolata for Powerline:style=Medium
xterm*faceName:     terminus:pixelsize=18

xpdf.viKeys:        true

#include "/Users/myuser/.dotfiles/x11/colors/tango"
! #include "/Users/myuser/.dotfiles/x11/colors/jwr"
#include "/Users/myuser/.dotfiles/x11/modules/font"
#include "/Users/myuser/.dotfiles/x11/modules/rxvt_perl"
#include "/Users/myuser/.dotfiles/x11/modules/rxvt_keysym"
#include "/Users/myuser/.dotfiles/x11/modules/xcalc"
#include "/Users/myuser/.dotfiles/x11/modules/xmisc"
#include "/Users/myuser/.dotfiles/x11/modules/osxterm"

URxvt.url-select.launcher:  /Users/myuser/bin/xdg-open
URxvt.clipboard.copycmd:    /usr/bin/pbcopy
URxvt.clipboard.pastecmd:   /usr/bin/pbpaste

urxvt*termName:             rxvt-unicode
scratchpad*font:            xft:terminus:pixelsize=14,xft:Inconsolata for Powerline:style=Medium
scratchpad*termName:        rxvt-unicode
scratchpad*background:      #313031
scratchpad*foreground:      White
float*font:                 xft:terminus:pixelsize=14,xft:Inconsolata for Powerline:style=Medium
float*termName:             rxvt-unicode
float*background:           #313031
float*foreground:           White
float*geometry:             120x28

urxvt*geometry:             179x51
urxvt*background:           #313031
urxvt*foreground:           White
urxvt*tintColor:            White
URxvt*transparent:          true
! urxvt*shading:              20    !trans, 0=opaque
urxvt*shading:              20
! urxvt*fading:               50    !term unfocus
urxvt*cursorColor:          #d8d8d8

! URxvt (scrollstyle:  rxvt (default), plain, next, or xterm)
! URxvt*title:             Terminal
URxvt.saveLines:         1500
URxvt*scrollBar:         false
URxvt*scrollColor:       #2A2A2A
URxvt*scrollstyle:       plain
URxvt*scrollBar_right:   true
URxvt.scrollKey:         true
URxvt.scrollWithBuffer:  true
URxvt.scrollBack:        true
URxvt.jumpScroll:        true
URxvt.skipScroll:        true
URxvt.urgentOnBell:      true
URxvt.trippleclickwords: true
URxvt.colorIT:           #605D5D
URxvt.colorBD:           #F9F9F9
URxvt.colorUL:           #FFA905
URxvt.colorRV:           #16596A
URxvt*cursorBlink:       false
URxvt*cursorUnderline:   false


! Colorscheme based on the gvim colorscheme desert
! xterm*termName:     xterm-256color
UXTerm*geometry: 140x36
!XTerm*underLine: on
UXTerm*customization: -color
!XTerm*visualBell: true
UXTerm*colorMode: on
UXTerm*colorBD: lightyellow
!XTerm*colorBDMode: off
UXTerm*colorUL: yellow
UXTerm*colorULMode: on
UXTerm*dynamicColors: off
UXTerm*highlightSelection: true

! Colorscheme based on the gvim colorscheme desert
UXTerm*background: #313031
UXTerm*foreground: White

!set the font, and fond size
! this also uses a antialias/scalable font
!xterm*faceName: Bitstream Vera Sans Mono
! xterm*faceName: Terminus
! xterm*faceSize: 10
! xterm*font:     xft:terminus:pixelsize=18
! xterm*font : -*-terminus-medium-*-*-*-12-*-*-*-*-*-iso8859-15
