Xcursor.theme: Adwaita

xman*topBox:false
xman*bothShown:true

! enable 96dpi fonts
!Xft.dpi: 90
!Xft.antialias: false

xpdf.viKeys: true

! xosview settings
xosview*background: black
xosview*foreground: Orange

! urxvt*depth: 24
urxvt*geometry:          120x40
urxvt*background:        #313031
urxvt*foreground:        White
